{
  "openapi": "3.0.1",
  "info": {
    "title": "GigDebugLoggerAPI",
    "version": "1.0"
  },
  "paths": {
    "/gettoken": {
      "get": {
        "tags": [
          "GigDebugLoggerAPI"
        ],
        "summary": "Creates authorisation token guid",
        "description": "Creates a new token Guid that is used for further communication with the API",
        "operationId": "GetToken",
        "parameters": [
          {
            "name": "apikey",
            "in": "query",
            "description": "public key identifies the API user",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GuidResult"
                }
              }
            }
          }
        }
      }
    },
    "/logevent": {
      "post": {
        "tags": [
          "GigDebugLoggerAPI"
        ],
        "summary": "Logs an event",
        "description": "Logs an event",
        "operationId": "LogEvent",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "apikey",
                  "eventType",
                  "exception",
                  "message",
                  "pubkey"
                ],
                "type": "object",
                "properties": {
                  "apikey": {
                    "type": "string"
                  },
                  "pubkey": {
                    "type": "string"
                  },
                  "eventType": {
                    "type": "string"
                  },
                  "message": {
                    "type": "string",
                    "format": "binary"
                  },
                  "exception": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "apikey": {
                  "style": "form"
                },
                "pubkey": {
                  "style": "form"
                },
                "eventType": {
                  "style": "form"
                },
                "message": {
                  "style": "form"
                },
                "exception": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Result"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "GuidResult": {
        "type": "object",
        "properties": {
          "value": {
            "type": "string",
            "format": "uuid"
          },
          "errorCode": {
            "$ref": "#/components/schemas/LoggerErrorCode"
          },
          "errorMessage": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoggerErrorCode": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      },
      "Result": {
        "type": "object",
        "properties": {
          "errorCode": {
            "$ref": "#/components/schemas/LoggerErrorCode"
          },
          "errorMessage": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}